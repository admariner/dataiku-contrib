{
    "meta" : {
        "label" : "Pageviews of selected pages over time"
    },

    "readable" : true,
    "writable" : false,

    "params": [
        {
            "name": "pages",
            "label" : "Pages to follow",
            "type": "TEXTAREA",
            "description":"One page per line, in the form 'project page', where project is in the form 'fr.wikipedia'",
            "mandatory" : true
        },
        {
            "name": "begin_date",
            "type": "STRING",
            "label": "Begin date",
            "description" : "In the form YYYYMMDD",
            "defaultValue" : "20240801",
            "mandatory" : true
        },
        {
            "name": "end_date",
            "type": "STRING",
            "label": "End date",
            "description" : "In the form YYYYMMDD",
            "defaultValue" : "20250104",
            "mandatory" : true
        }
    ]
}
